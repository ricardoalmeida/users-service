    "graphql": "^15.4.0",
    "graphql-transform-federation": "^2.1.0",
    "nexus-plugin-prisma": "^0.26.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.11.0",
    "@types/node": "14.14.13",
    "env-cmd": "10.1.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.0.0",
    "typescript": "4.0.5"
  }
